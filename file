function show_artikel()
    {
        $result = $this->db->query("SELECT a.*, b.nama_kategori,c.nama_user
        FROM artikel AS a
        LEFT JOIN tbl_kategori AS b ON a.id_kategori=b.id_kategori 
        LEFT JOIN tbl_user AS c ON a.id_user=c.id_user 
        
        ");
        return $result;
    }
